{
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.tabSize": 2,
    "eslint.alwaysShowStatus": true,
    "eslint.packageManager": "yarn",
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "typescript.autoClosingTags": true,
    "javascript.autoClosingTags": true,
    "editor.linkedEditing": true,
}